# Staging Environment Variables
REACT_APP_ENV=staging
REACT_APP_API_URL=https://api-staging.baselog.jp
REACT_APP_APP_URL=https://staging.baselog.jp

# Firebase (Staging)
REACT_APP_FIREBASE_API_KEY=your-staging-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-staging-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-staging-project
REACT_APP_FIREBASE_STORAGE_BUCKET=your-staging-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-staging-sender-id
REACT_APP_FIREBASE_APP_ID=your-staging-app-id

# Google Analytics (Staging用の別ID)
REACT_APP_GA_MEASUREMENT_ID=G-STAGING123

# Sentry (Staging環境)
REACT_APP_SENTRY_DSN=https://staging@sentry.io/project-id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_SENTRY=true
REACT_APP_SHOW_ENV_BADGE=true